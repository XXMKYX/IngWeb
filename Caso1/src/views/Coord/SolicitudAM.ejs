<fieldset>
    <legend>DATOS ACADÉMICOS</legend>
    <div class="grid-dos-columnas">
        <label for="InputInstitucionM">Institución de tus estudios profesionales</label>
        <label for="pInputinstitucionM">País la institución</label>
        <input type="text" name="InstitucionM"  id="InputInstitucionM" placeholder="Institución"    value="<%=JsonBusquedaM.InstitucionM%>"  >
        <input type="text" name="pinstitucionM" id="pInputInstitucionM" placeholder="País"    value="<%=JsonBusquedaM.pinstitucionM%>"  >
    </div>
    <div class=""  id="formaMaestria1">
        <div class="grid-dos-columnas">
            <label for="InputcarreraM">¿En qué carrera estás titulado?</label>
            <input type="text" name="carreraM" id="InputcarreraM"  placeholder="Carrera"    value="<%=JsonBusquedaM.carreraM%>"  >
        </div>                            
    </div>

    
    

    <label for="InputxpPM">Experiencia Profesional:</label>
    <textarea name="xpPM" id="InputxpPM"   ><%=JsonBusquedaM.xpPM%></textarea   >
        
    <label for="InputxpDM">Experiencia Docente:</label>
    <textarea name="xpDM" id="InputxpDM"><%=JsonBusquedaM.xpDM%></textarea   >
    
    <label for="InputaniospropM">¿En qué año(s) has asistido a los cursos propedéuticos de este Departamento?</label>
    <input type="text" name="aniospropM" id="InputaniospropM" placeholder="Año(s) Propedeuticos" value="<%=JsonBusquedaM.aniospropM%>">

    <label for="InputmotivoM">Motivo por el cuál desea ingresar al Posgrado y a qué línea de investigación desea integrarse</label>
    <textarea name="motivoM" id=InputmotivoM><%=JsonBusquedaM.motivoM%></textarea>
        

</fieldset>

<fieldset>
    <legend>ANEXO</legend>
    <!--
    <label for="fechaAplicacionM">Fecha</label>
    <input type="date" id="fechaAplicacionaM">
    -->    
    <div class="" id="archivosMaestria">
        <input type="file" style="display: none;" name="archivosubido1M" id="Inputarchivosubido1M" accept="application/pdf" value="<%=JsonBusquedaM.archivosubido1M%>">                                    
    </div>
</fieldset>