<div class="VistaMiSolicitud contenedor" id="prev-mi-solicitud">

    <%if (maestria.length > 0) {%>
    <% maestria.forEach(function(pre){ %>
    <div class="MiSolicitud" id="vista/">
        <ul class="ListaMiSolicitud">
            <li class="entrada-solicitud">
                <span>Datos personales</span> 
            </li>
            <li class="entrada-solicitud">
                <span>Nombre</span> 
                <p name="emailVista"> <%= pre.nombre %></p>
            </li>
            <li class="entrada-solicitud">
                <span>Estado de la solicitud</span>
                <p name="estadoSolicitud"> Pendiente</p>
            </li> 
            <li class="entrada-solicitud">
                <input type="button" value="Visualizar" onclick = "(visualizar(this))" class="boton boton-verde">   
            </li>
         
        </ul>        
        
    </div>
    <% }) %>

    <%} else {%>
        <div class="card card-body text-center">
            <p>No se encontraron registros. <a href="/new-entry">Debes registrarte primero</a></p>
          </div>
          <%}%>   
    
</div>
<div class="div-mi-solicitud" id="vista-mi-solicitud" style="display: none;">
    <%- include('maestria') %>  
</div>