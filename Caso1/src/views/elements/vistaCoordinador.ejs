<div class="contenedor ">
    <table class="table contenedor contenido-centrado centrar-columnas columnas-10">
        <caption>Solicitudes Enviadas</caption>
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">CURP</th>
            <th scope="col">Nombre</th>
            <th scope="col">Tipo</th>
            <th scope="col">Estado de Validacion</th>
            <th scope="col">Validar</th>
          </tr>
        </thead>
        <tbody>
            <!-- Contenido de la tabla (se hara con js)-->
            <%let cont=1;%>
            <% maestria.forEach(function(alumno) { %>
            <tr>
                <!-- Ejemplo de contenido -->
                <th scope="row"><%=cont%></th>
                <%cont++;%>
                <td><%= alumno.curpM %></td>
                <td><%= alumno.nombreM %></td>
                <td><%= alumno.tipoM %></td>
                <td><%= alumno.validacionM %></td>
                <!-- Ejemplo de contenido<td><%= alumno.Validacion %></td> -->
                <td><a href="/ValidarM/<%=alumno.CURP%>" class="btn btn-success btn-block">
                  Validar
                </a></td>
              </tr>
              <%})%>
                <% doctorado.forEach(function(alumno) { %>
                <tr>
                    <!-- Ejemplo de contenido -->
                    <th scope="row"><%= cont%></th>
                    <%cont++;%>
                    <td><%= alumno.curpD %></td>
                    <td><%= alumno.nombreD %></td>
                    <td><%= alumno.tipoD %></td>
                    <td><%= alumno.validacionD %></td>
                    <!--<td><%= alumno.ValidacionD %></td>-->
                    <td><a href="/ValidarD/<%=alumno.CURPD %>" class="btn btn-success btn-block">
                      Validar
                    </a></td>
                  </tr>
                  <%})%>
        </tbody>
      </table>
</div>

